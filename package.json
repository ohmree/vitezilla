{
  "private": true,
  "scripts": {
    "build": "dotenv -c -- turbo run build",
    "dev": "dotenv -c -- turbo run dev",
    "format": "prettier --cache --write \"**/*.{{c,m,}{t,j}s,md,y{a,}ml,json}\"",
    "lint": "dotenv -c -- turbo run lint",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@vitezilla/eslint-config": "workspace:*",
    "@volar-plugins/prettier": "^1.1.5",
    "dotenv-cli": "^6.0.0",
    "embed-examples": "^1.0.0",
    "eslint": "^8.30.0",
    "eslint-define-config": "^1.12.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.1",
    "prettier-plugin-packagejson": "^2.3.0",
    "turbo": "^1.6.3",
    "typescript": "^4.9.4"
  },
  "packageManager": "pnpm@7.15.0",
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "eslint",
        "typescript"
      ]
    }
  }
}
